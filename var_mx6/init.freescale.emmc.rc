on init
    # link emmc node to /dev/block/by-name
    symlink   /dev/block/platform/soc0/soc/2100000.aips-bus/2190000.usdhc/by-name /dev/block/by-name
    symlink   /dev/block/platform/soc0/soc/2100000.aips-bus/2190000.usdhc/mmcblk0boot0 /dev/block/by-name/bootloader

    # Load persistent dm-verity state
	verity_load_state


