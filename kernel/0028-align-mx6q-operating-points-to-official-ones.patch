Subject: [PATCH 28/34] align mx6q operating points to official ones

---
 arch/arm/boot/dts/imx6q-var-dart.dts    | 23 -----------------------
 arch/arm/boot/dts/imx6q-var-som-vsc.dts | 24 ------------------------
 arch/arm/boot/dts/imx6q-var-som.dts     | 23 -----------------------
 3 files changed, 70 deletions(-)

diff --git a/arch/arm/boot/dts/imx6q-var-dart.dts b/arch/arm/boot/dts/imx6q-var-dart.dts
index 650c9d7..155ae6b 100644
--- a/arch/arm/boot/dts/imx6q-var-dart.dts
+++ b/arch/arm/boot/dts/imx6q-var-dart.dts
@@ -26,29 +26,6 @@
 };
 
 
-&cpu0 {
-			operating-points = <
-				/* kHz    uV */
-				1200000 1350000
-				996000  1250000
-				852000  1250000
-				792000  1175000
-				396000  1175000
-			>;
-			fsl,soc-operating-points = <
-				/* ARM kHz  SOC-PU uV */
-				1200000 1275000
-				996000	1250000
-				852000	1250000
-				792000	1175000
-				396000	1175000
-			>;
-
-			arm-supply = <&reg_arm>;
-			pu-supply = <&reg_pu>;
-			soc-supply = <&reg_soc>;
-};
-
 &i2c2 {
 	pmic: pfuze100@08 {
 		compatible = "fsl,pfuze100";
